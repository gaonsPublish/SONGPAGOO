@charset "utf-8";

body, input, select, textarea, button {font-family:'NotoSansKR_400',NotoSansKR,'돋움',Dotum,Helvetica,'Apple SD Gothic Neo',Sans-serif;font-size:14.5px;line-height:1.5;color:#333;}
a {text-decoration:none;color:#333;background-color:transparent;}
a:hover, a:focus {outline:0;}

#header_wrap, #container_wrap, #footer_wrap {min-width:1000px;position:relative;/*overflow:hidden;*/ }
#header_wrap { border-top:1px solid rgba(0,0,0,0.3); border-bottom:1px solid rgba(0,0,0,0.3); position:relative;}
#footer_wrap { background-color:#54565a;}
#header {height:100px;width:1110px;position:relative; top:0; bottom:0; left:0; right:0; margin:0 auto;z-index:99999;}
#container {width:1110px;position:relative; top:0; bottom:0; left:0; right:0; margin:0 auto;}
#footer {height:190px;min-width:1000px; position:relative; background-color:#54565a; }

/* popup */
#popup_wrap { position: relative; overflow:hidden;}
#popup img {}
.close a { position:absolute; bottom:10px; right:310px; cursor:pointer; }

/* utilmenu */
.utilmenu {position:absolute;right:0px;top:10px; }
.utilmenu li {float:left;padding:0 10px;background:url('/images/common/sep_util.jpg') 0 8px no-repeat;}
.utilmenu li:first-child {padding-left:0;background:none;}
.utilmenu li:last-child {padding-right:0;}
.utilmenu li a {font-size:0.9em;}

/* gooey-v */
/*#side_menu { position:absolute; width:100px;  top:200px; background-color:red; }*/
#gooey-v { }
#gooey-v .open-button {}
#gooey-v .gooey-menu-item {width:89px;height:89px;background:#4487ab; }
#gooey-v .gooey-menu-item:hover .box p {background-color:#4487ab;}
/*pos:r*/
#gooey-v .gooey-menu-item .box {position:absolute;display:table;width:89px;height:89px;}
#gooey-v .gooey-menu-item .box p {display:table-cell;vertical-align:middle;border-radius:50%;text-align:center;color:#fff;}


/* gnb */
#gnb_img_wrap { position:absolute; overflow:hidden;background-color:#f7f6f5; width:100%; top:100px;z-index:99999; }
#gnb_img { width:1920px; top:0px;position:relative; display:none;background-color:#f7f6f5;}
#header .logo img { margin-top:37px; margin-right:60px; }
#gnb { position:absolute; width:830px; height:48px;bottom:0px; right:-15px;  top:50px; }
#gnb a {display:inline-block; vertical-align:top; text-align:left; color:#575757;}
 
#gnb>li {height:48px; float:left; position:relative; padding:0 10px; text-align:center; font-size:17px; font-style:bold; }
#gnb>li:first-child { width:100px; } 
#gnb>li:nth-child(2) { width:140px;}
#gnb>li:nth-child(3) { width:130px; }
#gnb>li:nth-child(4) { width:140px; }
#gnb>li:nth-child(5) { width:105px; }
#gnb>li:last-child {width:100px; padding-right:0; }

.gnb_line { width:2px; height:15px; position:absolute; right:0; top:13px; background-color:#dfdfdf;}


/*yang gnb menu /// width: 잠깐뺌*/
#gnb>li>a {height:40px; line-height:40px; color:#4a4a4a;}
/*yang gnb menu에 hover했을때 글자색상 */
#gnb>li:hover>a, #gnb>li.on>a {color:#0066cc; }

/*yang 원래는 #f7f6f5*/
#gnb ul {position:absolute; top:50px; height:303px; display:none; width:100px; background-color: #f7f6f5;  }
#gnb ul.gnb1_sub {width:120px; left:0px;}
#gnb ul.gnb2_sub {width:160px; left:0px;}
#gnb ul.gnb3_sub {width:150px; left:0px;}
#gnb ul.gnb4_sub {width:160px; left:0px;}
#gnb ul.gnb5_sub {width:125px; left:0px;}
#gnb ul.gnb6_sub {width:120px; left:0px;}

.gnb_sub_line { width:1px; height:255px;background-color:#dfdfdf; position:absolute; right:0; top:25px;}

/*yang gnb 아래 전체 ul*/
#gnb ul a {width:100px; height:30px;color:#000; font-size:15px;}
/*yang 센터안내*/
#gnb ul.gnb1_sub .sub1 {width:77px; padding:0 0 0 23px; margin-top:20px; }
#gnb ul.gnb1_sub .sub2,#gnb ul.gnb1_sub .sub3,#gnb ul.gnb1_sub .sub4,#gnb ul.gnb1_sub .sub5,#gnb ul.gnb1_sub .sub6,#gnb ul.gnb1_sub .sub7,
#gnb ul.gnb1_sub .sub8,#gnb ul.gnb1_sub .sub9 {width:77px; padding:0 0 0 23px;}

/*yang 어린이집지원*/
#gnb ul.gnb2_sub .sub1 {width:129px; padding:0 0 0 10px; margin-top:20px; }
#gnb ul.gnb2_sub .sub2, #gnb ul.gnb2_sub .sub3, #gnb ul.gnb2_sub .sub4, #gnb ul.gnb2_sub .sub5, #gnb ul.gnb2_sub .sub6,
#gnb ul.gnb2_sub .sub7, #gnb ul.gnb2_sub .sub8, #gnb ul.gnb2_sub .sub9 {width:129px; padding:0 0 0 10px; }

/*yang 가정양육지원*/
#gnb ul.gnb3_sub .sub1 {width:100px; padding:0 0 0 9px; margin-top:20px; }
#gnb ul.gnb3_sub .sub2,#gnb ul.gnb3_sub .sub3,#gnb ul.gnb3_sub .sub4,#gnb ul.gnb3_sub .sub5 {width:100px; padding:0 0 0 9px;}

/*yang 행사 및 교육신청*/
#gnb ul.gnb4_sub .sub1 {width:130px; padding:0 0 0 17px; margin-top:20px; }
#gnb ul.gnb4_sub .sub2,#gnb ul.gnb4_sub .sub3  {width:130px; padding:0 0 0 17px;}

/*yang 커뮤니티*/
#gnb ul.gnb5_sub .sub1 {width:87px; padding:0 0 0 15px; margin-top:20px; }
#gnb ul.gnb5_sub .sub2,#gnb ul.gnb5_sub .sub3,#gnb ul.gnb5_sub .sub4,#gnb ul.gnb5_sub .sub5,#gnb ul.gnb5_sub .sub6,#gnb ul.gnb5_sub .sub7 {width:87px; padding:0 0 0 15px;}

/*yang 통합상담실*/
#gnb ul.gnb6_sub .sub1 {width:80px; padding:0 0 0 5px; margin-top:20px; }
#gnb ul.gnb6_sub .sub2,#gnb ul.gnb6_sub .sub3,#gnb ul.gnb6_sub .sub4  {width:80px; padding:0 0 0 5px;}
#gnb ul.gnb6_sub .sub3 { width:90px;  padding:0 0 0 14px; }

#gnb ul a:hover {color:#3f7abc;}

/* footer */
#footer .foot_menu { width:100%; height:55px; background-color:#54565a; border-bottom:1px solid #fff; }
#footer .foot_menu ul { width:100%; text-align:center;}
#footer .foot_menu ul li { position:relative;display:inline-block; font-size:15px; margin-right:35px; margin-top:15px; color:#fff; cursor:pointer; }
.foot_menu_line { width:1px; height:15px; background-color:rgba(255,255,255,0.5); position:absolute; right:-20px; top:5px;}

#footer .foot_content { width:1110px; position:relative; top:0; bottom:0; left:0; right:0; margin:0 auto; background-color:red; }

#footer .logo_foot { width:30%; height:125px; float:left; text-align:center;}
#footer .logo_foot img { margin-top:40px;}

#footer .addr { width:34%; height:125px; color:#fff; float:left; position:relative; }
#addr_text { width:100%; position:absolute; top:30px;font-weight:100; font-size:12px; display:block; padding-top:10px;}
/*#addr_text p:nth-child(1) { margin-top:10px;}*/
/*#footer .addr p:nth-child(1) {  color:#fff; font-style:bold; font-size:14px; } */
/*#footer .addr p:nth-child(2) { width:100%;}*/
/*#footer .addr p:nth-child(3) { width:100%;}*/
#footer .addr .sep_foot {margin:0 5px; color:#fff;}
#footer .copy {/*font-family:Tahoma, Helvetica, Roboto, 'Apple SD Gothic Neo', Arial, 'Century Gothic', CenturyGothic, Geneva, AppleGothic, sans-serif;*/}

form { width:35%; height:125px; float:left; }
form .addr_select { width:215px; height:37px; margin-top:40px; margin-left:90px; }
form input { margin-top:40px;}
.addr_select option { background-color:#54565a; color:#fff;}







